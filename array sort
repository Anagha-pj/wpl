<body>
<?php
        $names=array("Vishnu","Anusha","Nevin","Salman","Zera");
        echo"The names before sorting is :";echo"<br>";
        print_r($names);
        asort($names);
        echo"<br><br>";
        echo"The names afters asort is :";echo"<br>";
        print_r($names);
        arsort($names);
        echo"<br><br>";
        echo"The names afters arsort is :";echo"<br>";
        print_r($names); 
 ?>
</body>
